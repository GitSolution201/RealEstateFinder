{
  "expo": {
    "name": "RealEstateFinder",
    "slug": "RealEstateFinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/comfortcloud.png",
    "scheme": "realestate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hassemmehboob.RealEstateFinder",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to let you choose property images.",
        "NSCameraUsageDescription": "We need access to your camera to let you take property photos.",
        "NSMicrophoneUsageDescription": "We need access to your microphone for videos with sound."
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.hassemmehboob.RealEstateFinder",
      "buildNumber": "1",
      "associatedDomains": [],
      "usesIcloudStorage": false,
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1047819931489-htm0i26cp092aro0oj9117kb6bnjr55m"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/comfortcloud.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hassemmehboob.RealEstateFinder",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/comfortcloud.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "/assets/fonts/Rubik-Medium.ttf",
            "/assets/fonts/Rubik-Regular.ttf",
            "/assets/fonts/Rubik-light.ttf",
            "/assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
